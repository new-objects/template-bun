{
  "name": "template-bun",
  "version": "0.0.0",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "dev": "bun run --hot ./src/index.js",
    "lint": "bunx eslint src",
    "prepare": "husky install",
    "lint:fix": "bun run lint -- --fix",
    "prettier": "bunx prettier src --check",
    "prettier:fix": "bun run prettier -- --write",
    "format": "bun run prettier:fix && bun run lint:fix"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "express": "^4.18.2"
  },
  "devDependencies": {
    "eslint": "^8.51.0",
    "eslint-config-prettier": "^9.0.0",
    "husky": "^8.0.3",
    "prettier": "^3.0.3",
    "bun-types": "latest"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  }
}